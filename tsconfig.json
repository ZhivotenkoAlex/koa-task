{
    "compilerOptions":
      {
        "target": "es2018",
        "module": "commonjs",
        "outDir": "./dist/",
        "sourceMap": true,
        "allowJs": true,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,

      },
      "file":"app.ts",
      "include": ["src/**/*.ts"],
      "exclude": ["node_modules"]
  }